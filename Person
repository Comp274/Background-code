
public class Person 
{
	private String Firstname, Lastname, id, password;
	private int age;char gender;
	private String addressOne, addressTwo, city, state, zip;
	private String phone, email;
	private Boolean hasInsurance, isActive;
		
	public Person(String First, String Last, String ID, String Password, int Age, char Gender, String Phone, String Email)
	{
		Firstname=First;
		Lastname=Last;
		id=ID;
		password=Password;
		age=Age;
		gender=Gender;
		phone=Phone;
		email=Email;
		hasInsurance=false;
		isActive=true;
	}

}
