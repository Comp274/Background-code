
public class Person 
{
	public String Firstname, Lastname, id, password;
	public int age;
	public char gender;
	public String addressOne, addressTwo, city, state, zip;
	public String phone, email;
	public Boolean hasInsurance, isActive;
		
	public Person(String First, String Last, String ID, String Password, int Age, char Gender, String Phone, String Email)
	{
		Firstname=First;
		Lastname=Last;
		id=ID;
		password=Password;
		age=Age;
		gender=Gender;
		phone=Phone;
		email=Email;
		hasInsurance=false;
		isActive=true;
	}

}
