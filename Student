import java.io.IOException;


public class Student extends Person
{
	public double GPA;
	public String enrolledOne, enrolledTwo, enrollResult,GPAresult;
	public String FOS;
	public boolean isEligible;
	
	public Student(String First, String Last, String ID, String Password, String Age, String Gender, String Phone, String Email,String a) throws IOException
	{
		super(First,Last,ID,Password,Age,Gender,Phone,Email);
		FOS=a; 
		GPA=3;
		enrolledOne="needsenrolled";
		enrolledTwo="needsenrolled";
	}
	
	public void Enroll(String pick)
	{
		if (enrolledOne == "needsenrolled")
		{
			enrolledOne=pick;
			enrollResult ="Enrolled successfully in " + pick + " for class one";

		}
		else if (enrolledTwo == "needsenrolled")
		{

			enrolledTwo=pick;
			enrollResult ="Enrolled successfully in " + pick + " for class two";
		}
		else
		{
			enrollResult = "You have enough classes";
		}
	}
	
	public void Eligibility()
	{
		GPA= Math.random() * 4 + 1;
		
		if (GPA <1.0)
		{	
			GPAresult = "Your GPA is " + GPA + "....You cannot schedule classes.";
		}
		else
		{
			GPAresult = "Your GPA is " + GPA;
			isEligible= true;
		}
	}
	
	public int checkPick(String pick)
	{
		if(pick == enrolledOne)
		{

			return 1;
		}
		else
		{
			return 0;
		}
	}
}
